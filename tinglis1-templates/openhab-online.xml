<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>openhab-online</Name>
  <Description>openHAB - a vendor and technology agnostic open source automation software for your home.</Description>
  <Registry>https://hub.docker.com/r/openhab/openhab/</Registry>
  <Repository>openhab/openhab:amd64-online</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8443</HostPort>
        <ContainerPort>8443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>5555</HostPort>
        <ContainerPort>5555</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/etc/localtime</HostDir>
      <ContainerDir>/etc/localtime</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/openhab/conf/</HostDir>
      <ContainerDir>/openhab/conf</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/openhab/userdata/</HostDir>
      <ContainerDir>/openhab/userdata</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:8080/</WebUI>
  <Banner>https://raw.githubusercontent.com/tinglis1/docker-containers/master/openhab/openhab_banner.png</Banner>
  <Icon>https://raw.githubusercontent.com/tinglis1/docker-containers/master/openhab/openhab-logo-square.png</Icon>
  <Beta>False</Beta>
  <Category>HomeAutomation:</Category>
  <Date>2016-06-21</Date> 
  <Support></Support>
  <Project>https://hub.docker.com/r/openhab/openhab/</Project>
  </Container>
